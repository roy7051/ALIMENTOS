<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ALIMENTOS" parent="Theme.MaterialComponents.NoActionBar">
        <!-- Primary brand color. -->

        <item name="colorOnBackground">@color/white</item>

        <item name="colorPrimary">@color/purple_200</item>

        
        <item name="android:windowBackground">@color/white</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>



        <item name="android:textColor">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->

        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.ALIMENTOS.NoActionBar" parent="Platform.MaterialComponents.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="colorPrimaryDark">@color/grey</item>

    </style>

    <style name="Theme.ALIMENTOS.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ALIMENTOS.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>